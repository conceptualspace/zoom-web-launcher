{
  "manifest_version": 3,
  "name": "Zoom Web Launcher",
  "description": "Autolaunch zoom in browser window",
  "version": "1.0.1",
  "author": "dev@conceptualspace.net",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "*://*.zoom.us/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/*"],
      "js": ["arrive.min.js", "content_script.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "512": "img/icon512.png"
  }
}
