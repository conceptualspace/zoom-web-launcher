{
  "manifest_version": 2,
  "name": "Zoom Web Launcher",
  "description": "Autolaunch zoom in browser window",
  "default_locale": "en",
  "version": "1.0.0",
  "author": "dev@conceptualspace.net",
  "permissions": [
    "*://*.zoom.us/*",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://*.zoom.us/*"],
      "js": ["content_script.js"],
      "all_frames": true,
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "512": "img/icon512.png"
  }
}
